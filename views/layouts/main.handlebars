<!doctype html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">


  <title>Queue-Develop</title>
  <meta name="description" content="Web Development For Small Businesses">
  <meta name="keywords"
    content="Web Development Services, Custom Website Design, Buisness Website, Willaimston, MI, Michigan, SEO, Cloud Hosting">
    <link href="https://cdn.quilljs.com/1.3.6/quill.snow.css" rel="stylesheet">
  <link href="/assets/CSS/tailwind.css" rel="stylesheet">

  <link rel="icon" href="/assets/imgs/queue-logo-sm.png">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;700&display=swap">
  <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
  <link href="https://unpkg.com/aos@2.3.4/dist/aos.css" rel="stylesheet">
  <link
    href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Merriweather:wght@400;700&display=swap"
    rel="stylesheet">

    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet">

  <meta name="viewport" content="width=device-width, initial-scale=1.0">

</head>

<body class="bg-gray-100 text-gray-900">

  {{#if (includes currentPath "/admin")}}
  {{#if isLoggedIn }}
  {{!-- Render admin nav if user is logged in --}}
  {{> admin-nav }}
  {{else}}
  {{!-- Render regular nav if user is not logged in but accessing admin route --}}
  {{> nav }}
  {{/if}}
  {{else}}

  {{!-- Render regular nav for non-admin routes --}}
  {{> nav }}
  {{/if}}
  <div id="page-container">
     {{{body}}}
  </div>
 

  <footer class="bg-gray-800 text-gray-300 py-10">
    <div class="container mx-auto grid grid-cols-1 md:grid-cols-4 gap-8">
      <!-- Logo Section -->
      <div class="flex flex-col items-center md:items-start">
        <a href="/" class="mb-4">
          <img src="/assets/imgs/queue-white.png" alt="Queue Development Logo" class="w-64">
        </a>
        <p class="text-sm text-gray-400">Â© 2025 Queue Development, LLC. All rights reserved.</p>
      </div>

      <!-- Services Links -->
      <div>
        <h5 class="text-lg font-semibold text-df8327 mb-4">Services</h5>
        <ul class="space-y-2">
          <li><a href="/web-features" class="hover:text-df8327 transition">Website Design</a></li>
          <li><a href="/email-features" class="hover:text-df8327 transition">Email Features</a></li>
          <li><a href="/hosting" class="hover:text-df8327 transition">Hosting</a></li>
          <li><a href="/seo" class="hover:text-df8327 transition">SEO</a></li>
        </ul>
      </div>

      <!-- Info Links -->
      <div>
        <h5 class="text-lg font-semibold text-df8327 mb-4">Info</h5>
        <ul class="space-y-2">
          <li><a href="/about" class="hover:text-df8327 transition">About Us</a></li>
          <li><a href="/pricing" class="hover:text-df8327 transition">Pricing</a></li>
          <li><a href="/services" class="hover:text-df8327 transition">Services</a></li>
          <li><a href="/contact" class="hover:text-df8327 transition">Contact</a></li>
        </ul>
      </div>

      <!-- Newsletter -->
      <div>
        <h5 class="text-lg font-semibold text-primaryQ mb-4">Subscribe to our Newsletter</h5>
        <p class="text-sm text-gray-400 mb-4">Monthly digest of what's new and exciting from us.</p>
        <form id="newsletterForm" class="flex flex-col sm:flex-row gap-2">
          <input type="email" id="email" name="email" placeholder="Email address"
            class="px-4 py-2 w-full sm:w-auto rounded-md focus:outline-none focus:ring focus:ring-primaryQ">
          <button type="submit"
            class="relative inline-block bg-primaryQ text-white text-md font-semibold px-2 py-1 rounded-lg shadow-lg overflow-hidden group">
            <div
              class="absolute inset-0 bg-black transition-transform duration-500 rounded-lg scale-x-0 group-hover:scale-x-100 origin-left">
            </div>
            <span class="relative z-10 group-hover:text-primaryQLight transition-colors duration-300">
              Get Started
            </span>
          </button>
        </form>
        <p id="newsletterMessage" class="text-sm mt-2"></p>
      </div>
    </div>

    <!-- Footer Bottom Section -->
    <div class="mt-8 border-t border-gray-700 pt-4">
      <div class="container mx-auto flex flex-col md:flex-row justify-between items-center text-sm">
        <div class="flex space-x-4">
          <a href="/accessibility" class="hover:text-df8327 transition">Accessibility</a>
          <span>|</span>
          <a href="/terms" class="hover:text-df8327 transition">Terms and Conditions</a>
          <span>|</span>
          <a href="/privacy" class="hover:text-df8327 transition">Privacy Statement</a>
          <span>|</span>
          <a href="/policy" class="hover:text-df8327 transition">Policy</a>
        </div>
      </div>
    </div>
  </footer>

  <style>
    .text-df8327 {
      color: #df8327;
    }

    .bg-df8327 {
      background-color: #df8327;
    }
  </style>



  <script src="/assets/js/newsletter.js"></script>

  <script src="https://unpkg.com/aos@2.3.4/dist/aos.js"></script>
  <script>
    AOS.init();
  </script>

</body>
<style>
  html,
  body {
    overflow-x: hidden;
  }
</style>

</html>